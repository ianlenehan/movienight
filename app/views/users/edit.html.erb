<h1>Edit Profile</h1>

<%= form_for @user do |f| %>
<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :name_first %>
<%= f.text_field :name_first %>

<%= f.label :name_last %>
<%= f.text_field :name_last %>

<!-- <%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %> -->

<%= f.label :image %>
<%= f.url_field :image %>

<%= f.submit %>
<% end %>
